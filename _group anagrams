from collections import defaultdict
class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        anagrams=  defaultdict(list)
        for string in strs:
            string1="".join(sorted(string))
            print(string)
            anagrams[string1].append(string)
        return list(anagrams.values())
        
