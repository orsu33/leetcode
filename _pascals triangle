class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        finalresult=[]
        for row in range(1,numRows+1):
            ans =1
            result=[ans]
            for col in range(1,row):
                ans=ans*(row-col)
                ans=ans//(col)
                result.append(ans)
            finalresult.append(result)
        return finalresult
        
        
